{
  "name": "{{npmName}}",
  "version": "{{version}}",
  "description": "{{description}}",
  "author": "{{author}}",
  "scripts": {
    "start": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "screenshot": "ice-devtools screenshot",
    "prepublishOnly": "npm run build && npm run screenshot"
  },
  "files": [
    "src/",
    "build/",
    "public/",
    "screenshot.png"
  ],
  "dependencies": {
    "vue": "^2.6.6",
    "vue-router": "^3.0.2"
  },
  "devDependencies": {
    "@vue/cli-service": "^3.4.0",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.5.21"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "publishConfig": {
    "registry": "{{registry}}"
  },
  "scaffoldConfig": {
    "builder": "vue-cli-service",
    "name": "{{name}}",
    "title": "{{title}}",
    "categories": [
      "基础模板"
    ]
  }
}
